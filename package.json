    "url": "https://github.com/tzmanics/devteach-16-website"
  },
  "homepage": "https://github.com/tzmanics/devteach-16-website",
  "devDependencies": {
    "babel-core": "6.4.5",
    "babel-loader": "6.2.2",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "clean-webpack-plugin": "0.1.9",
    "css-loader": "0.23.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "history": "1.17.0",

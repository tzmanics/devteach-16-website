{
  "name": "devteach-react-website",
  "version": "0.0.0",
  "description": "An example react website for DevTeach16",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf public",
    "dev": "npm run clean && NODE_ENV=development webpack-dev-server --progress --hot --history-api-fallback",
    "postinstall": "cp client/config/example.js client/config/production.js && cp client/config/example.js client/config/development.js",
    "start": "npm run clean && NODE_ENV=production webpack --progress && cp -r sites-enabled public"
  },
